{
  "name": "sfhelper",
  "displayName": "SF Helper",
  "publisher": "shreejan",
  "description": "Salesforce Helper streamlines Salesforce development, offering swift access to key operations like deploying, retrieving, running tests, executing anonymous code, and managing debug logs.",
  "version": "0.3.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shresthashreejan/SFHelper.git"
  },
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sfhelper.deploy",
        "title": "SF Helper: Deploy Active File"
      },
      {
        "command": "sfhelper.deployFolder",
        "title": "SF Helper: Deploy Active Folder"
      },
      {
        "command": "sfhelper.retrieve",
        "title": "SF Helper: Retrieve Active File"
      },
      {
        "command": "sfhelper.retrieveFolder",
        "title": "SF Helper: Retrieve Active Folder"
      },
      {
        "command": "sfhelper.runTest",
        "title": "SF Helper: Run Active Test Class"
      },
      {
        "command": "sfhelper.executeAnonymousCode",
        "title": "SF Helper: Execute Anonymous Apex Code"
      },
      {
        "command": "sfhelper.monitorDebugLogs",
        "title": "SF Helper: Monitor Debug Logs"
      },
      {
        "command": "sfhelper.deleteDebugLogs",
        "title": "SF Helper: Delete All Debug Logs"
      },
      {
        "command": "sfhelper.openDropdown",
        "title": "SF Helper: Show All Options"
      }
    ],
    "keybindings": [
      {
        "command": "sfhelper.deploy",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.deployFolder",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.retrieve",
        "key": "ctrl+shift+3",
        "mac": "cmd+shift+3",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.retrieveFolder",
        "key": "ctrl+shift+4",
        "mac": "cmd+shift+4",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.runTest",
        "key": "ctrl+shift+5",
        "mac": "cmd+shift+5",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.executeAnonymousCode",
        "key": "ctrl+shift+6",
        "mac": "cmd+shift+6",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.monitorDebugLogs",
        "key": "ctrl+shift+7",
        "mac": "cmd+shift+7",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.deleteDebugLogs",
        "key": "ctrl+shift+8",
        "mac": "cmd+shift+8",
        "when": "editorTextFocus || terminalFocus"
      },
      {
        "command": "sfhelper.openDropdown",
        "key": "ctrl+'",
        "mac": "cmd+'",
        "when": "editorTextFocus || terminalFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/vscode": "^1.70.0"
  }
}